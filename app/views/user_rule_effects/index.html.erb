<p id="notice"><%= notice %></p>

<h1>User Rule Effects</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_rule_effects.each do |user_rule_effect| %>
      <tr>
        <td><%= link_to 'Show', user_rule_effect %></td>
        <td><%= link_to 'Edit', edit_user_rule_effect_path(user_rule_effect) %></td>
        <td><%= link_to 'Destroy', user_rule_effect, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

