<p id="notice"><%= notice %></p>

<h1>User Rules</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_rules.each do |user_rule| %>
      <tr>
        <td><%= link_to 'Show', user_rule %></td>
        <td><%= link_to 'Edit', edit_user_rule_path(user_rule) %></td>
        <td><%= link_to 'Destroy', user_rule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Rule', new_user_rule_path %>
